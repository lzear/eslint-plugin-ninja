{
  "name": "eslint-plugin-ninja",
  "description": "Optimized ESLint Rules for Elite Coders",
  "version": "0.0.7",
  "homepage": "https://www.dont.ninja",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzear/eslint-plugin-ninja.git"
  },
  "author": "lzear",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ninja",
    "ninja",
    "10x",
    "10x engineer"
  ],
  "scripts": {
    "build": "vite build",
    "docs:dev": "vitepress dev",
    "docs:generate": "eslint-doc-generator --rule-list-columns name,description,fixable,hasSuggestions,deprecated --path-rule-list readme.md --url-rule-doc https://www.dont.ninja/rules/{name} --url-configs https://www.dont.ninja/configs && eslint-doc-generator --rule-list-columns name,description,fixable,hasSuggestions,deprecated --rule-doc-title-format name --path-rule-list ./docs/rules/index.md --url-rule-doc /rules/{name} --url-configs /configs/ && prettier --write readme.md ./docs/rules/index.md",
    "docs:build": "vitepress build",
    "release": "pnpm release:check && pnpm release:version && pnpm release:publish",
    "release:check": "pnpm test && pnpm run build",
    "release:publish": "clean-publish",
    "release:version": "changelogen --output changelog.md --release --push",
    "test:format": "prettier --check \"**/*.{js,ts,json,md,yml}\"",
    "test:js": "eslint \"**/*.{js,ts}\"",
    "test:types": "tsc --noEmit --pretty",
    "test": "pnpm run /^test:/"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./configs/recommended": {
      "require": "./dist/configs/recommended.js",
      "import": "./dist/configs/recommended.mjs"
    },
    "./configs/all": {
      "require": "./dist/configs/all.js",
      "import": "./dist/configs/all.mjs"
    },
    "./package.json": "./package.json"
  },
  "packageManager": "yarn@1.22.19",
  "peerDependenciesMeta": {
    "astro-eslint-parser": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "peerDependencies": {
    "astro-eslint-parser": "^0.16.0",
    "eslint": ">=8.0.0",
    "svelte": ">=3.0.0",
    "svelte-eslint-parser": "^0.33.0",
    "vue-eslint-parser": ">=9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.11.0",
    "date-fns": "^2.30.0",
    "has-emoji": "^1.0.0",
    "is-even": "^1.0.0",
    "left-pad": "^1.3.0",
    "minimatch": "^9.0.3",
    "natural-compare-lite": "^1.4.0",
    "node-noop": "^1.0.0",
    "std-env": "^3.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@azat-io/eslint-config-typescript": "^1.6.0",
    "@azat-io/stylelint-config": "^0.1.0",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@fontsource/comic-mono": "^5.0.7",
    "@fontsource/jetbrains-mono": "^5.0.17",
    "@types/babel__helper-validator-identifier": "^7.15.2",
    "@types/eslint": "^8.44.7",
    "@types/lodash": "^4.14.201",
    "@types/natural-compare-lite": "^1.4.2",
    "@types/node": "^20.9.0",
    "@types/prettier-linter-helpers": "^1.0.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@typescript-eslint/rule-tester": "^6.11.0",
    "@typescript-eslint/types": "^6.11.0",
    "@vercel/node": "^3.0.9",
    "@vitest/coverage-v8": "^0.34.6",
    "ajv": "^8.12.0",
    "astro-eslint-parser": "^0.16.0",
    "changelogen": "^0.5.5",
    "clean-publish": "^4.2.0",
    "codemirror": "^6.0.1",
    "daisyui": "^4.0.5",
    "eslint": "^8.53.0",
    "eslint-doc-generator": "^1.6.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-perfectionist": "^2.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vitest": "^0.3.9",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "prettier-linter-helpers": "^1.0.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-gamut": "^1.3.3",
    "stylelint-order": "^6.0.3",
    "stylelint-plugin-logical-css": "^0.13.2",
    "svelte": "^4.2.3",
    "svelte-eslint-parser": "^0.33.1",
    "tailwindcss": "^3.3.5",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "vitepress": "1.0.0-rc.25",
    "vitest": "^0.34.6",
    "vue": "^3.3.8",
    "vue-codemirror": "^6.1.1"
  },
  "volta": {
    "node": "18.18.2"
  }
}
